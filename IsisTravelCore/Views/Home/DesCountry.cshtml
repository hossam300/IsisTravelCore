@model IsisTravelCore.Models.Domains.Country

@{
    ViewData["Title"] = "DesCountry";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section id="contact-us" class="t-p medida wow fadeInUp">
    <div class="container wow fadeIn" data-wow-delay=".5s">
        <!-- container -->
        <div class="container">
            <div class="row">
                <div id="center_column" class="col-lg-12 col-md-12">
                    <img src="@Url.Content("~/uploads/"+Model.Name+"/"+Model.CountryMainImage)" width="100%" height="400px" />
                    <p>@Html.Raw(Model.Description) </p>
                </div>
            </div>
        </div>
    </div>
</section>